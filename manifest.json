{
  "name": "Sounds of nature",
  "version": "0.1",
  "background": {
    "scripts": ["bg.js"]
  },
  "minimum_chrome_version": "16.0.884",
  "permissions": [
      "<all_urls>",
      "background",
      "tabs",
      "bookmarks",
      "http://*/*",
      "https://*/*"
  ],
  "content_scripts": [ {
    "matches": ["http://*/*", "https://*/*"],
    "js": ["popup.js"],
    "all_frames": true
  }],
  "browser_action": {"default_popup": "popup.html"},
  "manifest_version": 2
}

