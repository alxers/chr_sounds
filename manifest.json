{
    "manifest_version": 2,

    "name": "Sounds of nature",
    "version": "0.1",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["bg.js", "popup.js"],
        "persistent": false
    },
    "permissions": [
        "<all_urls>"
    ]
}
